{
  "name": "todo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "setup": "npm i && npm run dev",
    "build": "npx tsc && npx tailwindcss -i ./public/stylesheets/style.css -o ./public/stylesheets/main.css",
    "dev": "concurrently \"npx tsc --watch\" \" npx tailwindcss -i ./public/stylesheets/style.css -o ./public/stylesheets/main.css --watch \" \"nodemon -q dist/bin/www\\",
    "start": "node ./dist/bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.1",
    "concurrently": "^8.2.0",
    "daisyui": "^3.1.7",
    "nodemon": "^2.0.22",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3"
  },
  "author": "Shakeel Faraz (https://github.com/sf-dev-git)"
}
