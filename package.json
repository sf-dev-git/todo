{
  "name": "todo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "setup": "npm i && npm run dev",
    "build": "npx tailwindcss -i ./public/stylesheets/style.css -o ./public/stylesheets/main.css",
    "dev": "concurrently \" npx tailwindcss -i ./public/stylesheets/style.css -o ./public/stylesheets/main.css --watch \" \"nodemon -q bin/www\"",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.1"
  },
  "author": "Shakeel Faraz (https://github.com/sf-dev-git)"
}
